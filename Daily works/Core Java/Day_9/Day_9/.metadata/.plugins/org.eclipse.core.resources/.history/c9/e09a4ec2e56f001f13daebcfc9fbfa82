package org.example.association.lib;

import java.util.Calendar;

public class Date {
	private int day;
	private int month;
	private int year;

	public Date() {
		Calendar c = Calendar.getInstance();
		this.day = c.get(Calendar.DAY_OF_MONTH);
		this.month = c.get(Calendar.MONTH) + 1;
		this.year = c.get(Calendar.YEAR);
	}
}
